   </extension>
   <extension
         point="hxeclipse.core.librarySelectors">
      <library-selector
            class="hxeclipse.haxelib.ui.dialogs.HaxeLibrarySelectorFactory"
            id="hxeclipse.haxelib.librarySelector">
      </library-selector>
   </extension>

</plugin>
